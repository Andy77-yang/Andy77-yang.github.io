one:\n","    print(\"\\n===== CLOSING CONNECTION =====\")\n","    client.close()\n","    print(\"Connection closed.\")\n","    print(\"MongoDB Atlas tutorial completed successfully!\")\n","else:\n","    print(\"\\nTutorial could not be completed due to connection failure.\")\n","    print(\"Please check your connection string and network settings.\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"V2lTg-_rx16x","executionInfo":{"status":"ok","timestamp":1745490725369,"user_tz":-60,"elapsed":45,"user":{"displayName":"Leonardo Alves Dias","userId":"06687564981830721914"}},"outputId":"63986d8c-1571-4bd0-9d84-03845cbfc8c6"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["\n","===== CLOSING CONNECTION =====\n","Connection closed.\n","MongoDB Atlas tutorial completed successfully!\n"]}]}]}